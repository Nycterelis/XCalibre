@model XCalibre.Models.AdminProjectViewModel
    @using (Html.BeginForm())
    {
        

        <h2>Edit Project User</h2>



        @Html.AntiForgeryToken()
        @Html.HiddenFor(model => model.Projects.Id)

        <div class="form-group">
            @Html.LabelFor(model => model.Name, htmlAttributes: new { @class = "control-label col-md-2" })
            @Html.ValidationMessageFor(model => model.Name, "", new { @class = "text-danger" })


            <div class="col-md-10">
                @Html.DisplayFor(model => model.Name)
            </div>
        </div>
        <div class="form-group" name="TempUrl" value="@ViewBag.SaveUrl" hidden>
            
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                @Html.ListBox("SelectedUsers", Model.User)
               
               <button class="btn btn-primary" type="submit">Assign Users</button>
            </div>
        </div>

    }